{
    "name": "besynes",
    "version": "0.1.0",
    "description": "Ferret Desktop Manager that lets you write and test FQL scripts",
    "main": "dist/index.js",
    "scripts": {
        "start": "cross-env ELECTRON_IS_DEV=1 electron dist/main/main.js",
        "dev": "electron-webpack dev",
        "compile": "electron-webpack",
        "clean": "rimraf dist/",
        "fmt": "pretty-quick"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/MontFerret/besynes.git"
    },
    "keywords": [
        "ferret",
        "fql",
        "desktop",
        "manager"
    ],
    "author": "MontFerret Team",
    "license": "Apache-2.0",
    "bugs": {
        "url": "https://github.com/MontFerret/besynes/issues"
    },
    "homepage": "https://github.com/MontFerret/besynes#readme",
    "electronWebpack": {
        "title": true,
        "main": {
            "sourceDirectory": "src/main"
        },
        "renderer": {
            "sourceDirectory": "src/renderer",
            "template": "src/renderer/index.html",
            "webpackConfig": "./webpack.renderer.additions.js"
        }
    },
    "dependencies": {
        "antd": "^3.21.2",
        "codemirror": "^5.48.2",
        "electron-is-dev": "^1.1.0",
        "parcel-bundler": "^1.12.3",
        "react": "^16.8.6",
        "react-codemirror2": "^6.0.0",
        "react-dom": "^16.8.6",
        "react-split-pane": "^0.1.87"
    },
    "devDependencies": {
        "@types/chai": "^4.1.7",
        "@types/mocha": "^5.2.7",
        "@types/node": "^12.6.8",
        "@types/react": "^16.8.23",
        "@types/react-dom": "^16.8.5",
        "@types/sinon": "^7.0.13",
        "autoprefixer": "^9.6.1",
        "chai": "^4.2.0",
        "cpy-cli": "^2.0.0",
        "cross-env": "^5.2.0",
        "css-loader": "^3.2.0",
        "electron": "^6.0.1",
        "electron-builder": "^21.2.0",
        "electron-webpack": "^2.7.4",
        "electron-webpack-ts": "^3.2.0",
        "husky": "^3.0.1",
        "mocha": "^6.2.0",
        "postcss-modules": "^1.4.1",
        "prettier": "^1.18.2",
        "pretty-quick": "^1.11.1",
        "rimraf": "^2.6.3",
        "sinon": "^7.3.2",
        "source-map-support": "^0.5.12",
        "style-loader": "^1.0.0",
        "ts-node": "^8.3.0",
        "tslint": "^5.18.0",
        "tslint-config-airbnb": "^5.11.1",
        "tslint-config-prettier": "^1.18.0",
        "typescript": "^3.5.3",
        "webpack": "^4.39.1"
    }
}
